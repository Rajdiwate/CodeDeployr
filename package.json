{
  "name": "CodeDeployr",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "prepare": "husky install",
    "format:check": "prettier --check \"**/*.{ts,tsx,md}\"",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "db:generate": "cd packages/db && pnpx prisma generate",
    "db:migrate": "cd packages/db && pnpx prisma migrate dev",
    "db:studio": "cd packages/db && pnpx prisma studio",
    "db:apply": "cd packages/db && pnpx prisma migrate deploy",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "turbo": "^2.5.5",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@9.0.0"
}
